{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Program",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 3000,
      "localRoot": "${workspaceRoot}/lambda",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false,
      "sourceMapPathOverrides": {
        "webpack:///./~/*": "${workspaceRoot}/lambda/node_modules/*",
        "webpack:///./*": "${workspaceRoot}/lambda/*",
        "webpack:///*": "*"
      }
    }
  ]
}